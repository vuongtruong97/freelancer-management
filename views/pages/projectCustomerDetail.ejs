<div class="card">
<div class="table-responsive text-nowrap">
    <table class="table">
      <thead>
        <tr>
          <th>Tên khách hàng</th>
          <th>Tên dự án</th>
          <th>Người quản lý</th>
          <th>Khoản ứng trước</th>
          <th>Ngân sách</th>
          <th>Chờ thanh toán</th>
          <th>Ngày bắt đầu</th>
          <th>Ngày kết thúc</th>
          <th>Trạng thái</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody class="table-border-bottom-0">
        <% data.forEach(function(item) { %>
          <tr>
            <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= item.accountName%></strong></td>
            <td><%= item.projectName%></td>
            <td>
            <%= item.manageName%>
            </td>
            <td>
             
                <table style="border:1px solid black">
                  <th  style="border:1px solid red;">Lần 1</th>
                  <th style="border:1px solid red;">Lần 2</th>
                  <th style="border:1px solid red;">Lần 3</th>
                  <tr>
                    <td style="border:1px solid red;"><%= item.advanceMoney.firstTime%></td>
                  
                    <td style="border:1px solid red;"><%= item.advanceMoney.secondTime%></td>
                 
                    <td style="border:1px solid red;"><%= item.advanceMoney.lastTime%></td>
                  </tr>
                </table>
            </td>
            <td><%=item.totalMoney%></td>
            <td><%=item.totalMoney -item.advanceMoney.firstTime - item.advanceMoney.secondTime - item.advanceMoney.lastTime  %></td>
            <td><%= item.startDate%></td>
            <td><%= item.endDate%></td>
            <td><span class="badge bg-label-primary me-1">Active</span></td>
            <td>
              <div class="dropdown">
                <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                  <i class="bx bx-dots-vertical-rounded"></i>
                </button>
                <div class="dropdown-menu">
                  <a class="dropdown-item" href="javascript:void(0);"
                    ><i class="bx bx-edit-alt me-1"></i> Cập nhật </a
                  >
                  <!-- <a class="dropdown-item" href="javascript:void(0);"
                    ><i class="bx bx-trash me-1"></i> Delete</a
                  > -->
                </div>
              </div>
            </td>
          </tr>
        <%})%>
      
      </tbody>
    </table>
  </div>
</div>